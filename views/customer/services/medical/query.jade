extends ../../layout
block pageStyle
  link(rel='stylesheet', href='/stylesheets/customer/services/medical.css')
block pageContent
  a.back-btn(href='/services/medical') BACK
  section.section5
    .blank
    .inner
      .header
        .header-back
          h3
            | CONSULTA EN
            br
            | CONSULTORIO
          .comment
            p ¿Desea acudir al médico? Vango! Le recomienda lo más accesible, hace la cita y cubre el costo de la consulta. Para Vango! su salud es primordial.
    .bottom
      a.main-button(href='#')
        ¡Obtén cobertura ya!

block pageScript
  script.
    $('.navbar').addClass('top');
    $('.navbar .btn-consult').hide();
    $('.navbar .btn-social').show();
    $( window ).scroll(function() {
      var scroll = $(window).scrollTop();
      if(scroll < 10){
        $('.navbar .btn-consult').hide();
        $('.navbar .btn-social').show();
      }else{
        $('.navbar .btn-consult').show();
        $('.navbar .btn-social').hide();

      }
    });

    $('.nav-services').addClass('active');
    $('.logo-large').attr('src', '/images/customer/services/logo_lg.png');
    $('.logo-medium').attr('src', '/images/customer/services/logo_md.png');
    $('.logo-small').attr('src', '/images/customer/services/logo_sm.png');

    $('#blogmenu').attr('href', 'https://consultorio.segurosvango.com/');
