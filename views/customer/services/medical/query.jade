extends ../../layout
block pageStyle
  link(rel='stylesheet', href='/stylesheets/customer/services/medical.css')
block pageContent
  a.back-btn(href='/services/medical') BACK
  section.section5
    .blank
    .inner
      .header
        h3
          | CONSULTA EN
          br
          | CONSULTORIO
        p VALLA AL DOCTOR / SU ASISTENCIA VANGO CUBRE LOS GASTOS DE LA CONSULTA MÉDICA / ENTRE A LA PLATAFORMA Y SELECCIONE LA APLICACIÓN “CONSULTA EN CONSULTORIO / NOSOTROS HAREMOS LA CITA Y EL PAGO DE SU CONSULTA / PARA VANGO SU SALUD ES PRIMERO
    .bottom
      a.main-button(href='#')
        ¡Obtén cobertura ya!

block pageScript
  script.
    $('.navbar').addClass('top');
    $('.navbar .btn-consult').hide();
    $('.navbar .btn-social').show();
    $( window ).scroll(function() {
      var scroll = $(window).scrollTop();
      if(scroll < 10){
        $('.navbar .btn-consult').hide();
        $('.navbar .btn-social').show();
      }else{
        $('.navbar .btn-consult').show();
        $('.navbar .btn-social').hide();

      }
    });

    $('.nav-services').addClass('active');
    $('.logo-large').attr('src', '/images/customer/services/logo_lg.png');
    $('.logo-medium').attr('src', '/images/customer/services/logo_md.png');
    $('.logo-small').attr('src', '/images/customer/services/logo_sm.png');

    $('#blogmenu').attr('href', 'https://consultaconsultorio.segurosvango.com');
