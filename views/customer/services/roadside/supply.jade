extends ../../layout
block pageStyle
  link(rel='stylesheet', href='/stylesheets/customer/services/roadside.css')
block pageContent
  a.back-btn(href='/services/roadside') BACK
  section.section8
    .blank
    .inner
      .header
        .header-back
          h3
            | suministro gasolina
            br
            | y líquidos
          .comment
            p ¿Necesitas gasolina o los líquidos de tu coche? ¡Protege la bomba de tu auto! Obtén gasolina a domicilio con Vango! Paga únicamente el combustible y por nuestra cuenta corre llenar a nivel los demás fluidos de: motor, frenos, dirección, transmisión, limpiaparabrisas y limpiafaros.
    .bottom
      a.main-button(href='#')
        ¡Obtén cobertura ya!

block pageScript
  script.
    $('.navbar').addClass('top');
    $('.navbar .btn-consult').hide();
    $('.navbar .btn-social').show();
    $( window ).scroll(function() {
      var scroll = $(window).scrollTop();
      if(scroll < 10){
        $('.navbar .btn-consult').hide();
        $('.navbar .btn-social').show();
      }else{
        $('.navbar .btn-consult').show();
        $('.navbar .btn-social').hide();

      }
    });

    $('.nav-services').addClass('active');
    $('.logo-large').attr('src', '/images/customer/services/logo_lg.png');
    $('.logo-medium').attr('src', '/images/customer/services/logo_md.png');
    $('.logo-small').attr('src', '/images/customer/services/logo_sm.png');

    $('#blogmenu').attr('href', 'https://gasolinaadomicilio.segurosvango.com');
