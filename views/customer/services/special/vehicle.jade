extends ../../layout
block pageStyle
  link(rel='stylesheet', href='/stylesheets/customer/services/special.css')
block pageContent
  a.back-btn(href='/services/special') BACK
  section.section4
    .blank
    .inner
      .header
        .header-back
          h3
            | gestoría
            br
            | vehícular
          .comment
            p Los trámites relacionados con el vehículo como cambio de placas, alta ó baja del padrón vehicular, tenencias, infracciones, multas, liberación de vehículos, entre otros. Vango! Hace la gestión por tí. Tu tiempo y  tranquilidad nos apremia.
    .bottom
      a.main-button(href='#')
        ¡Obtén cobertura ya!

block pageScript
  script.
    $('.navbar').addClass('top');
    $('.navbar .btn-consult').hide();
    $('.navbar .btn-social').show();
    $( window ).scroll(function() {
      var scroll = $(window).scrollTop();
      if(scroll < 10){
        $('.navbar .btn-consult').hide();
        $('.navbar .btn-social').show();
      }else{
        $('.navbar .btn-consult').show();
        $('.navbar .btn-social').hide();

      }
    });

    $('.nav-services').addClass('active');
    $('.logo-large').attr('src', '/images/customer/services/logo_lg.png');
    $('.logo-medium').attr('src', '/images/customer/services/logo_md.png');
    $('.logo-small').attr('src', '/images/customer/services/logo_sm.png');

    $('#blogmenu').attr('href', 'https://gestoriavehicular.segurosvango.com');
