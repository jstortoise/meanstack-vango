extends ../../layout
block pageStyle
  link(rel='stylesheet', href='/stylesheets/customer/services/special.css')
block pageContent
  a.back-btn(href='/services/special') BACK
  section.section1
    .blank
    .inner
      .header
        h3
          | GESTORÍA
          br
          | ADMINISTRATIVA
        p NO DE MILES DE VUELTAS, SI NO SABE QUÉ REQUISTOS PIDEN PARA TRAMITAR PASAPORTE, IFE, LICENCIA ETC. / CON SU ASISTENCIA “GESTORÍA ADMINISTRATIVA” EL ASESOR DE TRÁMITES LO SACARA DE CUALQUIER DUDA / VALLA A LO SEGURO.
    .bottom
      a.main-button(href='#')
        ¡Obtén cobertura ya!

block pageScript
  script.
    $('.navbar').addClass('top');
    $('.navbar .btn-consult').hide();
    $('.navbar .btn-social').show();
    $( window ).scroll(function() {
      var scroll = $(window).scrollTop();
      if(scroll < 10){
        $('.navbar .btn-consult').hide();
        $('.navbar .btn-social').show();
      }else{
        $('.navbar .btn-consult').show();
        $('.navbar .btn-social').hide();

      }
    });

    $('.nav-services').addClass('active');
    $('.logo-large').attr('src', '/images/customer/services/logo_lg.png');
    $('.logo-medium').attr('src', '/images/customer/services/logo_md.png');
    $('.logo-small').attr('src', '/images/customer/services/logo_sm.png');

    $('#blogmenu').attr('href', 'https://gestoria.segurosvango.com');
