extends ../../layout
block pageStyle
  link(rel='stylesheet', href='/stylesheets/customer/services/travel.css')
block pageContent
  a.back-btn(href='/services/travel') BACK
  section.section4
    .blank
    .inner
      .header
        h3
          | renta de
          br
          | auto
        p SE QUERDÓ SIN AUTO ¿PERO NECESITA COUNTINUAR SU VIAJE? / SIN PROBLEMAS, VANGO LE RENTA UN AUTO EN CASO DE ROBO O AVERÍA DE SU VEHÍCULO / SIN COSTO ALGUNO Y DE MANERA IMMEDIATA.
    .bottom
      a.main-button(href='#')
        ¡Obtén cobertura ya!

block pageScript
  script.
    $('.navbar').addClass('top');
    $('.navbar .btn-consult').hide();
    $('.navbar .btn-social').show();
    $( window ).scroll(function() {
      var scroll = $(window).scrollTop();
      if(scroll < 10){
        $('.navbar .btn-consult').hide();
        $('.navbar .btn-social').show();
      }else{
        $('.navbar .btn-consult').show();
        $('.navbar .btn-social').hide();

      }
    });

    $('.nav-services').addClass('active');
    $('.logo-large').attr('src', '/images/customer/services/logo_lg.png');
    $('.logo-medium').attr('src', '/images/customer/services/logo_md.png');
    $('.logo-small').attr('src', '/images/customer/services/logo_sm.png');

    $('#blogmenu').attr('href', 'https://rentacar.segurosvango.com');
