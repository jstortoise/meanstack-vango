extends ../../layout
block pageStyle
  link(rel='stylesheet', href='/stylesheets/customer/services/travel.css')
block pageContent
  section.section5
    .blank
    .inner
      .header
        h3 transportación
        p NO QUEDE INMOVILIZADO / CON LA ASISTENCIA “PAGO DE TRANSPORTE POR ROBO O AVERÍA DE VEHÍCULO” LLEGARÁ A SU DESTINO / USANDO LA ÚLTIMA TECNOLOGÍA LE ENVIAREMOS SU CLAVE DE RESERVACIÓN Y UBICACIÓN DE LA TERMINAL MÁS CERCANA.
    .bottom
      a.main-button(href='#')
        ¡Obtén cobertura ya!

block pageScript
  script.
    $('.navbar').addClass('top');
    $('.navbar .btn-consult').hide();
    $('.navbar .btn-social').show();
    $( window ).scroll(function() {
      var scroll = $(window).scrollTop();
      if(scroll < 10){
        $('.navbar .btn-consult').hide();
        $('.navbar .btn-social').show();
      }else{
        $('.navbar .btn-consult').show();
        $('.navbar .btn-social').hide();

      }
    });

    $('.nav-services').addClass('active');
    $('.logo-large').attr('src', '/images/customer/services/logo_lg.png');
    $('.logo-medium').attr('src', '/images/customer/services/logo_md.png');
    $('.logo-small').attr('src', '/images/customer/services/logo_sm.png');

    $('#blogmenu').attr('href', 'https://transportacion.segurosvango.com');
