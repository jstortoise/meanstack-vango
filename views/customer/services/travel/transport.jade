extends ../../layout
block pageStyle
  link(rel='stylesheet', href='/stylesheets/customer/services/travel.css')
block pageContent
  a.back-btn(href='/services/travel') BACK
  section.section5
    .blank
    .inner
      .header
        .header-back
          h3 transportación
          .comment
            p ¡No interrumpas tu viaje! Si tu vehículo se averió, te transportamos con tus acompañantes. Vango! contratará los servicios de transporte privado ó autobús de primera clase, nos interesa que continúes tu camino.
      .mobile-comment
        p ¡No interrumpas tu viaje! Si tu vehículo se averió, te transportamos con tus acompañantes. Vango! contratará los servicios de transporte privado ó autobús de primera clase, nos interesa que continúes tu camino.
    .bottom
      a.main-button(href='#')
        ¡Obtén cobertura ya!

block pageScript
  script.
    $('.navbar').addClass('top');
    $('.navbar .btn-consult').hide();
    $('.navbar .btn-social').show();
    $( window ).scroll(function() {
      var scroll = $(window).scrollTop();
      if(scroll < 10){
        $('.navbar .btn-consult').hide();
        $('.navbar .btn-social').show();
      }else{
        $('.navbar .btn-consult').show();
        $('.navbar .btn-social').hide();

      }
    });

    $('.nav-services').addClass('active');
    $('.logo-large').attr('src', '/images/customer/services/logo_lg.png');
    $('.logo-medium').attr('src', '/images/customer/services/logo_md.png');
    $('.logo-small').attr('src', '/images/customer/services/logo_sm.png');

    $('#blogmenu').attr('href', 'https://transportacion.segurosvango.com');
